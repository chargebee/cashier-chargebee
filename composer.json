{
    "name": "laravel/cashier-chargebee",
    "description": "Laravel Cashier for Chargebee provides an expressive, fluent interface to Chargebee's subscription billing services.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Laravel\\CashierChargebee\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\CashierChargebee\\CashierServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Padraig Nolan",
            "email": "padraig@threadable.io"
        },
        {
            "name": "Anna Karykowska",
            "email": "anna@threadable.io"
        }
    ],
    "require": {
        "illuminate/support": "^11.37",
        "illuminate/database": "^11.37"
    },
    "require-dev": {
        "orchestra/testbench": "^8.18|^9.0",
        "phpunit/phpunit": "^10.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\CashierChargebee\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "packagist.org": false,
        "private-packagist": {
            "type": "composer",
            "url": "https://repo.packagist.com/c4cb/"
        }
    }
}
