{
    "name": "laravel/cashier-chargebee",
    "description": "Laravel Cashier for Chargebee provides an expressive, fluent interface to Chargebee's subscription billing services.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Laravel\\CashierChargebee\\": "src/",
            "Laravel\\CashierChargebee\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\CashierChargebee\\CashierServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Padraig Nolan",
            "email": "padraig@threadable.io"
        },
        {
            "name": "Anna Karykowska",
            "email": "anna@threadable.io"
        },
        {
            "name": "Tomasz Mrozi≈Ñski",
            "email": "tomasz@threadable.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "moneyphp/money": "^4.0",
        "chargebee/chargebee-php": "^3.38",
        "dompdf/dompdf": "^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.18|^9.0",
        "phpunit/phpunit": "^10.4",
        "phpstan/phpstan": "^1.10"
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\CashierChargebee\\Tests\\": "tests/",
            "Laravel\\CashierChargebee\\Tests\\Fixtures\\": "tests/Fixtures/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "packagist.org": false,
        "private-packagist": {
            "type": "composer",
            "url": "https://repo.packagist.com/c4cb/"
        }
    }
}
